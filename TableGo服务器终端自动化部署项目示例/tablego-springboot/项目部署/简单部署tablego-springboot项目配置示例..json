{
  "isOpenTerminal" : true,
  "isOutLogToTerminal" : true,
  "projectPath" : "G:\\tablego-springboot",
  "scriptPath" : "G:\\tablego-springboot\\项目部署",
  "serverProjectPath" : "/home/test/tablego-springboot",
  "batchConfig" : [ {
    "runtimeExec" : [ {
      "command" : "${scriptPath}\\svn-update-project.bat",
      "enable" : false
    }, {
      "command" : "${scriptPath}\\maven-lifecycle.bat clean ${projectPath}",
      "enable" : true
    }, {
      "command" : "${scriptPath}\\maven-lifecycle.bat \"clean package\" ${projectPath}\\tablego-admin",
      "enable" : true
    } ]
  }, {
    "fileUploads" : [ {
      "fromPath" : [ "${projectPath}\\tablego-admin\\target\\tablego-admin.jar" ],
      "toPath" : "${serverProjectPath}",
      "enable" : true
    } ]
  }, {
    "commands" : [ {
      "command" : "cd ${serverProjectPath}",
      "enable" : true
    }, {
      "command" : "sh AppService.sh restart tablego-admin 6611",
      "sleepSeconds" : 3
    }, {
      "command" : "tail -f tablego-admin.log",
      "waitSigquitSeconds" : 2
    }, {
      "command" : "ls"
    } ]
  } ]
}