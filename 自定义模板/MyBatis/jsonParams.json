    "T_PROVINCE" : [ "PROVINCE_NAME", "PROVINCE_CODE" ],
    "T_CITY" : [ "CITY_NAME", "CITY_CODE", "PARENT_CODE" ],
    "T_AREA" : [ "AREA_NAME", "AREA_CODE", "PARENT_CODE" ],
    "T_ROLE" : [ "ROLE_NAME", "ROLE_CODE" ]
