{
  "projectDir" : "D:\\tablego-mybatis",
  "basePackagePath" : "cn.tablego.mybatis",
  "srcPath" : "/src/main/java",
  "resourcesPath" : "/src/main/resources",
  "commonFields" : "CREATED_BY, CREATION_DATE, LAST_UPDATED_BY, LAST_UPDATE_DATE, DELETE_FLAG",
  "ftlParamList" : [ {
    "templateName" : "${tableUpperCamelCase}[java].ftl",
